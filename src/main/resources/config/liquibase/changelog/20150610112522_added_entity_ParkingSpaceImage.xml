<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <property name="now" value="now()" dbms="mysql,h2"/>
    <property name="now" value="current_timestamp" dbms="postgresql"/>

    <!--
        Added the entity ParkingSpaceImage.
    -->
    <changeSet id="20150610112522" author="jhipster">
        <createTable tableName="PARKINGSPACEIMAGE">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="image" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            <column name="parkingspace_id" type="bigint"/>
        </createTable>
        
        <addForeignKeyConstraint baseColumnNames="parkingspace_id"
                                 baseTableName="PARKINGSPACEIMAGE"
                                 constraintName="fk_parkingspaceimage_parkingspace_id"
                                 referencedColumnNames="id"
                                 referencedTableName="PARKINGSPACE"/>

    </changeSet>
</databaseChangeLog>
